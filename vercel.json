{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "crons": [
        {
            "path": "/assets/cron/cron.js",
            "schedule": "24 11 * * *"

        }
    ]

}